          SQLTask:SqlStatementSource="INSERT INTO FACT_DAYS_TO_BILL &#xA;SELECT MONTH_DIM_SID, &#xA;       LOCATION_DIM_SID,&#xA;       AR_CATEGORY_DIM_SID,&#xA;       DECODE(GROUPING(BILL_CLASS_DIM_SID), 1, -200, BILL_CLASS_DIM_SID) BILL_CLASS_DIM_SID,&#xA;       DECODE(GROUPING(VA_FACILITY_TYPE_DIM_SID), 1, -400, VA_FACILITY_TYPE_DIM_SID) VA_FACILITY_TYPE_DIM_SID,&#xA;       SUM(TOTAL_BILLS)TOTAL_BILLS,&#xA;       SUM(TOTAL_DAYS)TOTAL_DAYS,&#xA;       SUM(TOTAL_BILLINGS)TOTAL_BILLINGS,&#xA;       SUM(DAYS_NET_EMRA)DAYS_NET_EMRA,&#xA;       SUM(TOTAL_BILLS_FYTD)TOTAL_BILLS_FYTD,&#xA;       SUM(TOTAL_DAYS_FYTD)TOTAL_DAYS_FYTD,&#xA;       SUM(DAYS_NET_EMRA_FYTD)DAYS_NET_EMRA_FYTD,&#xA;       SUM(TOTAL_BILLINGS_FYTD)TOTAL_BILLINGS_FYTD,&#xA;       SUM(TOTAL_DAYS_GT_90) ,  &#xA;       SUM(TOTAL_DAYS_GT_90_FYTD) TOTAL_DAYS_GT_90_FYTD, &#xA;       SUM(TOTAL_BILLS_GT_90) TOTAL_BILLS_GT_90, &#xA;       SUM(TOTAL_BILLS_GT_90_FYTD) TOTAL_BILLS_GT_90_FYTD&#xA;  FROM stg_days_to_bill S,&#xA;       DIM_AR_CATEGORY C,&#xA;       DIM_BILL_CLASS B,&#xA;       DIM_LOCATION L,&#xA;       DIM_VA_FACILITY_TYPE F,&#xA;       bio_Bis_etl_log&#xA; WHERE S.STA3N = L.STATION_ID&#xA;   AND'TOTAL - '||SUBSTR(S.AR_CATEGORY_TYPE,1,1) =  SUBSTR(UPPER(C.AR_CATEGORY_TYPE), 1, 9)&#xA;   AND NVL(S.BILL_CLASS_DESCRIPTION, 'Unknown') = B.BILL_CLASS_DESCRIPTION &#xA;   AND NVL(VA_NONVA, 'U') = SUBSTR(F.VA_FACILITY_TYPE,1,1)&#xA;GROUP BY LOCATION_DIM_SID,&#xA;       AR_CATEGORY_DIM_SID,&#xA;       MONTH_DIM_SID,&#xA;  CUBE (BILL_CLASS_DIM_SID,&#xA;       VA_FACILITY_TYPE_DIM_SID)" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
