    <DTS:Executable
      DTS:refId="Package\Load Azure STGBillings"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{5AF0D634-CCF5-45C7-A5F3-E702EA897878}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Load Azure STGBillings"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2012; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="1">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{26D5014D-0E2A-4F02-9ADD-AB0E591B9A06}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="&#xA;WITH getbillings AS &#xA;(&#x9;SELECT  &#xA;&#x9;&#x9;a.billnumber ,&#xA;&#x9;&#x9;originalbillamount, &#xA;&#x9;&#x9;bill_class_code, &#xA;&#x9;&#x9;sta3n,&#xA;&#x9;&#x9;ROW_NUMBER() OVER (PARTITION BY a.AccountsReceivableSID ORDER BY VISTAEDITDATE DESC) RNK,&#xA;&#x9;&#x9;ARCategoryDimSID,&#xA;&#x9;&#x9;CASE WHEN ISNULL(originalbillamount,0) &lt; 51 THEN 1&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--'&lt;= $51'&#xA;&#x9;&#x9;&#x9; WHEN originalbillamount &gt;= 51 and originalbillamount &lt; 250   THEN 2    --'&gt; $51 and &lt; $250'&#xA;             WHEN originalbillamount &gt;= 250 and originalbillamount &lt; 1500 THEN 3    --'&gt;= $250 and &lt; $1500'&#xA;             WHEN originalbillamount &gt;= 1500                           THEN 4       --'&gt;= $1500'&#xA;        END RANGE_ID,&#xA;        CASE WHEN isnull(fmsaccountingfund, 'X') = '528711' THEN 3&#xA;&#x9;         WHEN isnull(fmsaccountingfund, 'X')&lt;&gt; '528711' THEN &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CASE WHEN bill_class_code in ('1','2') THEN 1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; WHEN bill_class_code in ('3','4') THEN 2&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; ELSE -99 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;END  &#xA;&#x9;&#x9;END bill_class_dim_sid, &#xA;&#x9;&#x9;CASE WHEN RevenueSourceCode IN ('8014', '8017', '8018', '8028', '8029', '8030' ,'8035')&#xA;&#x9;&#x9;&#x9;&#x9;THEN RevenueSourceCode &#xA;&#x9;&#x9;&#x9; ELSE  '-99' &#xA;&#x9;&#x9;END RevenueSourceCode, &#xA;&#x9;&#x9;authorization_date,&#xA;&#x9;&#x9;arcategory, &#xA;&#x9;&#x9;MonthDimSID month_id&#xA;&#x9;FROM  &#xA;&#x9;&#x9;biobis.fact_ar_track_change_ALLhist a&#xA;&#x9;&#x9;JOIN biobis.DimARCategory&#xA;&#x9;&#x9;&#x9;ON arcategory = ARCategoryDescription&#xA;&#x9;&#x9;JOIN biobis.BioBisETLLog &#xA;&#x9;&#x9;&#x9;ON CONVERT(DATE, billprepareddatetime) BETWEEN MonthBeginDate AND MonthEndDate&#xA;&#x9;WHERE opcode IN ('U', 'I')&#xA;&#x9;AND ARCategoryType in ('THIRD PARTY', 'MISC', 'OTHER')&#xA;&#x9;AND begin_date &lt;= DATEADD(DAY,1,MonthEndDate)&#xA; )&#xA;&#xA; INSERT INTO biobisstg.STGBillings&#xA; (&#xA;&#x9;MonthDimSID&#xA;    ,STA3N&#xA;    ,RangeID&#xA;    ,BillClassDimSID&#xA;    ,ARCategoryDimSID&#xA;    ,RevenueSourceCode&#xA;    ,NumBills&#xA;    ,AmtBills&#xA; )&#xA; SELECT &#xA;&#x9;month_id MONTH_DIM_SID ,&#xA;&#x9;STA3N, &#xA;&#x9;RANGE_ID, &#xA;&#x9;bill_class_dim_sid, &#xA;&#x9;ARCATEGORYdimsid,&#xA;&#x9;revenuesourcecode, &#xA;&#x9;COUNT(BILLNUMBER) NUM_BILLS, &#xA;&#x9;SUM(originalbillamount) AMT_BILLS&#xA; FROM &#xA;&#x9;GETBILLINGS B --, DIM_MONTH M&#xA; WHERE &#xA;&#x9;RNK = 1&#xA;&#x9;AND charindex('-',substring(BillNumber,5,16)) = 0 &#xA;&#x9;AND bill_class_code IS NOT NULL&#xA; GROUP BY &#xA;&#x9;month_id,&#xA;&#x9;bill_class_dim_sid, &#xA;&#x9;STA3N,&#xA;&#x9;ARCATEGORYdimsid, &#xA;&#x9;RANGE_ID, &#xA;&#x9;revenuesourcecode&#xA;ORDER BY &#xA;&#x9;STA3N, &#xA;&#x9;bill_class_dim_sid;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
