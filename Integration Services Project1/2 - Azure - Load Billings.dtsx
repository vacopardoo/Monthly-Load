            <component
              refId="Package\Get Bill  Adjustments\Read 1P Adjustments, ReEstablish Records"
              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="Read 1P Adjustments, ReEstablish Records"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH get_transactions  AS
(
SELECT 
	month_id, 
	accountsreceivablesid,
	a.sta3n,  
	transactiondatetime, 
	transactionamount ,
	transactionnumber, 
	principaamountcollected, 
	artransactiontype tran_artransactiontype, 
	artransactionsid
FROM 
	cdwwork.IB.ARTRANSACTION a, 
	BIO_BIS.DFLT.etl_log , 
	cdwwork.dim.ARTransactionType t
WHERE 
	CONVERT(DATE, TRANSACTIONDATETIME)  BETWEEN month_begin_date AND month_end_date
	AND a.artransactiontypesid = t.artransactiontypesid
)



--Using monthly transactions, Get 'INCREASE ADJUSTMENT'/'RE-ESTABLISH' Transactions 

SELECT 
	accountsreceivablesid, 
	SUM(transactionamount) transactionamount  --get establish trans
FROM 
	cdwwork.ib.ARTRANSACTION k, 
	cdwwork.dim.ARTransactionType t
WHERE  
	k.ARTransactionTypeSID = t.ARTransactionTypeSID
	and artransactiontype in ('INCREASE ADJUSTMENT','RE-ESTABLISH')
	and exists (SELECT '1' 
		FROM get_transactions j
		WHERE j.ARTransactionSID = k.ARTransactionSID)--current month transactons
GROUP BY 
	accountsreceivablesid;</property>
