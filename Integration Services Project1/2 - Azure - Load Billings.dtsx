          SQLTask:Connection="{26D5014D-0E2A-4F02-9ADD-AB0E591B9A06}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="&#xA;--Get 3rd Party and Misc Billings&#xA;WITH getbillings AS &#xA;(&#xA;&#x9;SELECT &#xA;&#x9;&#x9;MonthDimSID month_dim_sid, &#xA;&#x9;&#x9;a.billnumber,&#xA;&#x9;&#x9;originalbillamount,&#xA;&#x9;&#x9;bill_class_code, &#xA;&#x9;&#x9;sta3n,&#xA;&#x9;&#x9;ROW_NUMBER() OVER (PARTITION BY a.AccountsReceivableSID ORDER BY VISTAEDITDATE DESC) RNK,&#xA;&#x9;&#x9;ARCategoryDimSID,&#xA;&#x9;&#x9;CASE WHEN ISNULL(originalbillamount,0) &lt; 51 THEN 1&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--'&lt;= $51'&#xA;             WHEN originalbillamount &gt;= 51 AND originalbillamount &lt; 250   THEN 2&#x9;--'&gt; $51 and &lt; $250'&#xA;             WHEN originalbillamount &gt;= 250 AND originalbillamount &lt; 1500 THEN 3&#x9;--'&gt;= $250 and &lt; $1500'&#xA;             WHEN originalbillamount &gt;= 1500 THEN 4&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--'&gt;= $1500'&#xA;        END RANGE_ID,&#xA;        CASE WHEN ISNULL(fmsaccountingfund, 'X') = '528711' then 3&#xA;&#x9;&#x9;&#x9; WHEN ISNULL(fmsaccountingfund, 'X')&lt;&gt; '528711' then &#xA;&#x9;&#x9;&#x9;&#x9;CASE WHEN bill_class_code IN ('1','2') then 1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9; WHEN bill_class_code IN ('3','4') then 2&#xA;&#x9;&#x9;&#x9;&#x9;ELSE -99 &#xA;&#x9;&#x9;&#x9; END  &#xA;        END bill_class_dim_sid,&#xA;&#x9;&#x9;RevenueSourceCode, &#xA;&#x9;&#x9;authorization_date,&#xA;&#x9;&#x9;arcategory&#xA;&#x9;FROM  &#xA;&#x9;&#x9;biobis.fact_ar_track_change_ALLhist a&#xA;&#x9;&#x9;JOIN biobis.DimARCategory&#xA;&#x9;&#x9;&#x9;ON arcategory = ARCategoryDescription&#xA;&#x9;&#x9;JOIN biobis.BioBisETLLog --DIM_DATE&#xA;&#x9;&#x9;&#x9;ON  CONVERT(DATE, BILLPREPAREDDATETIME) between FiscalYearBeginDate and MonthEndDate&#xA;&#x9;WHERE &#xA;&#x9;&#x9;OpCode IN ('U', 'I')&#xA;&#x9;&#x9;AND ARCategoryType IN ('THIRD PARTY', 'MISC', 'OTHER')&#xA;--&#x9;&#x9;AND FiscalYear = 2020&#xA;&#x9;&#x9;AND begin_date &lt;= DATEADD(DAY,1,MonthEndDate)&#xA;&#x9;&#x9;AND CONVERT(DATE, BILLPREPAREDDATETIME) &lt; DATEADD(DAY,1,MonthEndDate)&#xA; )&#xA;&#xA;INSERT  INTO biobisstg.STGBillingsFYTD&#xA;(&#xA;&#x9;MonthDimSID&#xA;    ,STA3N&#xA;    ,RangeID&#xA;    ,BillClassDimSID&#xA;    ,ARCategoryDimSID&#xA;    ,Revenuesourcecode&#xA;    ,NumBillsFYTD&#xA;    ,AmtBillsFYTD&#xA;)&#xA;SELECT &#xA;&#x9;MONTH_DIM_SID,&#xA;&#x9;STA3N, &#xA;&#x9;RANGE_ID, &#xA;&#x9;bill_class_dim_sid, &#xA;&#x9;ARCategoryDimSID, &#xA;&#x9;revenuesourcecode,&#xA;&#x9;COUNT(BILLNUMBER) NUM_BILLS_FYTD, &#xA;&#x9;SUM(originalbillamount) AMT_BILLS_FYTD&#xA;FROM &#xA;&#x9;GETBILLINGS B&#xA;WHERE &#xA;&#x9;RNK = 1&#xA;&#x9;AND CHARINDEX('-',SUBSTRING(BillNumber,5,16)) = 0 &#xA;&#x9;AND bill_class_code IS NOT NULL&#xA;GROUP BY &#xA;&#x9;MONTH_DIM_SID,&#xA;&#x9;bill_class_dim_sid, &#xA;&#x9;STA3N,&#xA;&#x9;ARCategoryDimSID, &#xA;&#x9;RANGE_ID, &#xA;&#x9;revenuesourcecode&#xA;ORDER BY STA3N, bill_class_dim_sid;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
