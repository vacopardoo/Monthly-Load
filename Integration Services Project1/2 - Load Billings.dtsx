      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{D84925D7-49BF-407B-94C2-C5077DE8DBC9}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="&#xA;--Get 3rd Party and Misc Billings&#xA;with GETBILLINGS as (SELECT &#xA;month_id month_dim_sid, &#xA;(a.billnumber) ,([originalbillamount]), [bill_class_code], sta3n,&#xA;row_number() over (PARTITION BY a.AccountsReceivableSID ORDER BY VISTAEDITDATE DESC) RNK,&#xA;AR_CATEGORY_dim_sid,&#xA;&#x9; CASE WHEN ISNULL([originalbillamount],0) &lt; 51                       THEN 1       --'&lt;= $51'&#xA;                 WHEN [originalbillamount] &gt;= 51 and [originalbillamount] &lt; 250   THEN 2       --'&gt; $51 and &lt; $250'&#xA;                 WHEN [originalbillamount] &gt;= 250 and [originalbillamount] &lt; 1500 THEN 3       --'&gt;= $250 and &lt; $1500'&#xA;                 WHEN [originalbillamount] &gt;= 1500                           THEN 4       --'&gt;= $1500'&#xA;         END RANGE_ID,&#xA;&#xA;         CASE WHEN isnull(fmsaccountingfund, 'X') = '528711' then 3&#xA;&#x9;      WHEN isnull(fmsaccountingfund, 'X')&lt;&gt; '528711' then &#xA;&#x9;&#x9; case when bill_class_code in ('1','2') then 1&#xA;              when bill_class_code in ('3','4') then 2&#xA;&#x9;&#x9;&#x9;  else -99 end  &#xA;          end bill_class_dim_sid,RevenueSourceCode, authorization_date,&#xA;arcategory&#xA;FROM  BIO_BIS.Dflt.fact_ar_track_change_ALLhist a&#xA;join BIO_BIS.DFLT.dim_ar_category&#xA;on    ARCATEGORY = AR_CATEGORY_DESCRIPTION&#xA;JOIN BIO_BIS.DFLT.etl_log --DIM_DATE&#xA;ON  CONVERT(DATE, BILLPREPAREDDATETIME) between fiscal_year_begin_date and month_end_date&#xA;WHERE OPCODE IN ('U', 'I')&#xA; AND AR_CATEGORY_TYPE in ('THIRD PARTY', 'MISC', 'OTHER')&#xA; and fiscal_year = 2020&#xA; and begin_date &lt;= month_end_load_date&#xA; AND CONVERT(DATE, BILLPREPAREDDATETIME) &lt; month_end_load_date&#xA; )&#xA;&#xA;insert  into stg_billings_fytd&#xA; SELECT MONTH_DIM_SID ,STA3N, RANGE_ID, bill_class_dim_sid, AR_CATEGORY_dim_sid, &#xA; COUNT(BILLNUMBER) NUM_BILLS_FYTD, SUM(originalbillamount) AMT_BILLS_FYTD,&#xA; revenuesourcecode&#xA;FROM GETBILLINGS B&#xA;WHERE RNK = 1&#xA;AND charindex('-',substring(BillNumber,5,16)) = 0 &#xA;and bill_class_code is not null&#xA;GROUP BY MONTH_DIM_SID,bill_class_dim_sid, &#xA;STA3N,AR_CATEGORY_dim_sid, RANGE_ID, revenuesourcecode&#xA;ORDER BY STA3N, bill_class_dim_sid;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
